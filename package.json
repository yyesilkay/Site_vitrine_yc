{
  "name": "site-vitrine-yc",
  "version": "0.0.0",
  "scripts": {
    "ng": "nx",
    "start": "nx serve apps/frontend",
    "start:api": "nx serve apps/api",
    "start:all": "nx run-many --target=serve --projects=apps/frontend,apps/api --parallel",
    "build": "set NX_CLOUD=false && nx build apps/frontend",
    "build:api": "set NX_CLOUD=false && nx build apps/api",
    "build:all": "set NX_CLOUD=false && nx run-many --target=build --projects=frontend,api",
    "build:frontend:prod": "nx build frontend --configuration=production",
    "build:api:prod": "nx build api --configuration=production",
    "build:prod": "npm run build:frontend:prod && npm run build:api:prod",
    "build:prod:githubpages": "nx build --output-path docs --base-href /Site_vitrine_yc/ --configuration production && xcopy /E /Y docs\\browser\\* docs\\ && rmdir /S /Q docs\\browser",
    "watch": "nx build --watch --configuration development",
    "test": "nx test",
    "serve:ssr:frontend": "node dist/apps/frontend/server/server.mjs",
    "lint": "nx affected:lint",
    "lint:fix": "nx lint --fix",
    "prettier": "prettier . --ignore-unknown --write",
    "prettier:check": "prettier . --ignore-unknown --check"
  },
  "private": true,
